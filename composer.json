{
    "name": "supham/php-share",
    "description": "Sharing common used php package to multiple projects.",
    "keywords": ["composer", "shared library", "symlink"],
    "type": "composer-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Yusup Hambali",
            "email": "supalpuket@gmail.com"
        }
    ],
    "support": {
        "source": "https://github.com/sup-ham/php-share"
    },
    "require": {
        "composer-plugin-api": "*"
    },
    "require-dev": {
        "composer/composer": "*"
    },
    "autoload": {
        "psr-4": {
            "Supham\\Phpshare\\Composer\\": "src"
        }
    },
    "extra": {
        "class": "Supham\\Phpshare\\Composer\\Plugin"
    }
}
